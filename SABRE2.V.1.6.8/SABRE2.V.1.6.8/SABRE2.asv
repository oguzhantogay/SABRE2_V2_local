function bc_menu_Callback(hObject, eventdata)
    fprint
   clc; pan off; zoom off; rotate3d off; set([vzm,vrm,vpm], 'Checked', 'off');
   set(pt_title_name,'Visible','off') ;set(ri_infor_text,'Visible','off') 
   if strcmp(get(gcbo,'type'),'uimenu') 
      OBJ=get(gcbo,'Tag');
   end  
   SABRE2ModelR(JNodevalue,Massemble,JNodevalue_i,JNodevalue_j, ...
      Rval,BNodevalue,vrum_az_slider,vrum_el_slider,vrum_az_edit,vrum_el_edit,LabType,axesm,vstm);   
   % Drawing Nodal white points & generate NC
   if isequal(strcmp(get(vstm,'Checked'),'on'),1) % white background   
      [RNCc,Nshe1,Nshe2,DUP1,DUP2,NCc,Bhe1,Bhe2,The1,The2,SGhe1,SGhe2,Mhe1,Mhe2,Bhg,Thg,CSg,BNC1,BNC2,FEL,error]=SABRE2LBCODEW(JNodevalue,...
         Massemble,JNodevalue_i,JNodevalue_j,Rval,BNodevalue,SNodevalue,...
         RNCc,Nshe1,Nshe2,DUP1,DUP2,NCc,LNC,LUEC,PNC,BNC,BNC1,BNC2,FEL,Bhe1,Bhe2,The1,The2,SGhe1,SGhe2,Mhe1,Mhe2,Bhg,Thg,CSg,LabType,pt_title_name,axesm,vstm);     
   elseif isequal(strcmp(get(vstm,'Checked'),'on'),0) % black background       
      [RNCc,Nshe1,Nshe2,DUP1,DUP2,NCc,Bhe1,Bhe2,The1,The2,SGhe1,SGhe2,Mhe1,Mhe2,Bhg,Thg,CSg,BNC1,BNC2,FEL,error]=SABRE2LBCODE(JNodevalue,...
         Massemble,JNodevalue_i,JNodevalue_j,Rval,BNodevalue,SNodevalue,...
         RNCc,Nshe1,Nshe2,DUP1,DUP2,NCc,LNC,LUEC,PNC,BNC,BNC1,BNC2,FEL,Bhe1,Bhe2,The1,The2,SGhe1,SGhe2,Mhe1,Mhe2,Bhg,Thg,CSg,LabType,pt_title_name,axesm,vstm);    
   end  
   % Drawing M*S* 
   SABRE2AssiCODE(JNodevalue,Massemble,JNodevalue_i,JNodevalue_j,...
      Rval,BNodevalue,SNodevalue,axesm,vstm); 
   if isempty(SNodevalue) || ~isequal(error,0)
   else
      xn = sum(sum(SNodevalue(:,:,3)));   % Total number of Elements
      N = length(RNCc(:,1));   
      PNC(N,14)=0; PNC1(xn,14)=0; PNC2(xn,14)=0;
%       BNC(N,14)=0; BNC1(xn,14)=0; BNC2(xn,14)=0; 
   end    
   % set initial data automatically.Fixed
   set([bfm_type_name,bfm_coordx_edit,bfm_coordy_edit,bfm_coordz_edit],'String','')
   set([bfm_ux_edit,bfm_uy_edit,bfm_uz_edit,bfm_rx_edit,bfm_ry_edit,...
      bfm_rz_edit,bfm_phix_edit],'String','0')     
   set([bfm_ux_on_radiobutton,bfm_uy_on_radiobutton,bfm_uz_on_radiobutton,...
      bfm_rx_on_radiobutton,bfm_ry_on_radiobutton,bfm_rz_on_radiobutton,...
      bfm_phix_on_radiobutton],'value',1)
    set([bfm_ux_off_radiobutton,bfm_uy_off_radiobutton,bfm_uz_off_radiobutton,...
      bfm_rx_off_radiobutton,bfm_ry_off_radiobutton,bfm_rz_off_radiobutton,...
      bfm_phix_off_radiobutton],'value',0)    
   % set initial data automatically.Shear Panel
   if isempty(BNC1)
      nextshnode = 1;
   else
      nextshnode = length(BNC1(:,1))+1;
   end
   % Empty Edits
   set(bsm_member_name,'string','');
   set(bsm_type_name,'string',num2str(nextshnode));
   set([bsm_jointi_edit,bsm_jointj_edit,bsm_kv_edit],'String','');
   set(bsm_jointi_radiobutton,'value',1)
   set(bsm_jointj_radiobutton,'value',0)             
   % set initial data automatically.
   set([bgm_type_name,bgm_coordx_edit,bgm_coordy_edit,bgm_coordz_edit],'String','')
   set([bgm_ux_edit,bgm_uy_edit,bgm_uz_edit,bgm_rx_edit,bgm_ry_edit,bgm_rz_edit,bgm_phix_edit],'String','0') 
   set([vdum,vtum],'enable','off');set([vdum_panel,vdum_type_subpanel],'Visible','off')
   set([vdum_type_name,v1um_p1_text,v1um_p2_text,v1um_p3_text,v1um_p4_text,v1um_p5_text],'Visible','off') 
   set([v1um_p1_edit,v1um_p2_edit,v1um_p3_edit,v1um_p4_edit,v1um_p5_edit],'Visible','off')
